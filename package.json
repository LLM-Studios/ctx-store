{
	"name": "ctx-store",
	"version": "1.0.50",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"dev": "bun run --watch src/index.ts",
		"sync-openapi": "bun scripts/sync-openapi.ts",
		"build": "bun build --outfile ./dist/server.js ./src/index.ts",
		"build:executable": "bun build --compile --target bun --outfile server ./src/index.ts",
		"build:types": "tsc ./client/client.ts --declaration --emitDeclarationOnly --outDir ./dist --noEmitOnError --skipLibCheck",
		"build:client": "bun build ./client/client.ts --outdir ./dist --target node --format esm",
		"link": "bun run sync-openapi && bun build:types && bun build:client && bun link",
		"prepublish": "bun run sync-openapi && bun build:types && bun build:client",
		"----DATABASE----": "",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:deploy": "prisma migrate deploy",
		"db:generate": "prisma generate",
		"postinstall": "prisma generate",
		"----DOCKER----": "",
		"docker:build": "docker build -t llmstudios/ctx-store:latest .",
		"docker:push": "docker push llmstudios/ctx-store:latest",
		"docker:publish": "npm run docker:build && npm run docker:push"
	},
	"bin": {
		"ctx-store": "./dist/server.js"
	},
	"files": [
		"./dist/server.js",
		"./dist/client.js",
		"./dist/client.d.ts"
	],
	"dependencies": {
		"@elysiajs/eden": "^1.1.3",
		"@elysiajs/swagger": "^1.1.6",
		"@prisma/client": "5.22.0",
		"elysia": "^1.1.25",
		"prisma": "5.22.0"
	},
	"devDependencies": {
		"@hey-api/client-fetch": "^0.4.4",
		"@hey-api/openapi-ts": "^0.57.0",
		"@types/bun": "^1.1.13",
		"bun-types": "latest"
	},
	"exports": {
		".": {
			"import": "./dist/client.js",
			"types": "./dist/client.d.ts"
		}
	},
	"main": "./dist/client.js"
}
